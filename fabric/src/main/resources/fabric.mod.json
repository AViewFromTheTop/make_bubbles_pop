{
  "schemaVersion": 1,
  "id": "make_bubbles_pop",
  "version": "${version}-fabric",
  "name": "Make Bubbles Pop Mod",
  "description": "This client-side mod makes bubble particles pop and lets them realistically rise to the water surface.\nIt also adds bubble particles to opening chests and explosions underwater.",
  "authors": [
    "Tschipcraft"
  ],
  "contact": {
    "homepage": "https://linktr.ee/tschipcraft",
    "sources": "https://github.com/Tschipcraft/make_bubbles_pop",
    "issues": "https://github.com/Tschipcraft/make_bubbles_pop/issues"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.tschipcraft.make_bubbles_pop.MakeBubblesPop"
    ]
  },
  "mixins": [
    "make_bubbles_pop.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric": "*",
    "minecraft": ">=1.19.4",
    "java": ">=17"
  },
  "custom": {
    "catalogue": {
      "banner": "icon.png"
    }
  }
}
